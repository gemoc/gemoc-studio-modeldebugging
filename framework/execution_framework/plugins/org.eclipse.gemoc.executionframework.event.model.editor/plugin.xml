<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated event -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipse.gemoc.executionframework.event.model.event.presentation.EventModelWizardID"
            name="%_UI_EventModelWizard_label"
            class="org.eclipse.gemoc.executionframework.event.model.event.presentation.EventModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/EventModelFile.gif">
         <description>%_UI_EventModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated event -->
      <editor
            id="org.eclipse.gemoc.executionframework.event.model.event.presentation.EventEditorID"
            name="%_UI_EventEditor_label"
            icon="icons/full/obj16/EventModelFile.gif"
            extensions="event"
            class="org.eclipse.gemoc.executionframework.event.model.event.presentation.EventEditor"
            contributorClass="org.eclipse.gemoc.executionframework.event.model.event.presentation.EventActionBarContributor">
      </editor>
   </extension>

</plugin>
